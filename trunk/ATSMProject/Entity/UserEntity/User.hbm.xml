<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="JVL" assembly="UserEntity">
    <class name="User" table="User">
        <id name="Id">
            <column name="Id" not-null="true" />
            <generator class="assigned" />
        </id>
        <property name="Password">
            <column name="Password" />
        </property>
        <property name="RoleId">
            <column name="RoleId" />
        </property>
        <property name="InsertDate">
            <column name="InsertDate" />
        </property>
        <property name="InsertUserId">
            <column name="InsertUserId" />
        </property>
        <property name="UpdateDate">
            <column name="UpdateDate" />
        </property>
        <property name="UpdateUserId">
            <column name="UpdateUserId" />
        </property>
        <property name="DeleteDate">
            <column name="DeleteDate" />
        </property>
        <property name="DeleteUserId">
            <column name="DeleteUserId" />
        </property>
    </class>
</hibernate-mapping>
