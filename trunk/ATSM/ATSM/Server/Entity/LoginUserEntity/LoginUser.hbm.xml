<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="JVL" assembly="LoginUserEntity">
    <class name="LoginUser" table="LoginUser">
        <id name="LoginUserId">
            <column name="LoginUserId" not-null="true" />
            <generator class="assigned" />
        </id>
        <property name="Password">
            <column name="Password" />
        </property>
        <property name="InsertDate">
            <column name="InsertDate" />
        </property>
        <property name="UpdateDate">
            <column name="UpdateDate" />
        </property>
        <property name="DeleteDate">
            <column name="DeleteDate" />
        </property>
        <property name="DeleteUserId">
            <column name="DeleteUserId" />
        </property>
        <property name="NewPassword">
            <column name="NewPassword" />
        </property>
        <property name="Status">
            <column name="Status" />
        </property>
    </class>
</hibernate-mapping>
